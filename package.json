{
  "name": "gatsby-source-magento2",
  "description": "Gatsby source plugin for building websites using Magento as a data source.",
  "version": "1.0.0",
  "author": "Marc-Antoine Ruel <hello@marcantoineruel.com>",
  "bugs": {
    "url": "https://github.com/maru3l/gatsby-source-magento2/issues"
  },
  "homepage": "https://github.com/maru3l/gatsby-source-magento2#readme",
  "repository": "https://github.com/maru3l/gatsby-source-magento2",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "format": "prettier --write 'src/**/*.js' 'README.md'",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "magento"
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "axios": "^0.18.0",
    "babel-preset-gatsby-package": "^0.1.3",
    "cross-env": "^5.2.0",
    "prettier": "^1.16.4"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0",
    "gatsby-source-filesystem": "^2.0.20"
  }
}
